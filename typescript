  const loadData = async () => {
    try {
      // Load both need and offers
      const [needResponse, offersResponse] = await Promise.all([
        needsAPI.getById(needId),
        offersAPI.getByNeed(needId)
      ]);

      if (needResponse.success) {
        setNeed(needResponse.need);
      }

      if (offersResponse.success) {
        // Filter out original offers that have been countered
        // Show only: pending offers, accepted offers, declined offers, and counter offers
        const filteredOffers = (offersResponse.offers || []).filter(offer => {
          // If offer is countered, don't show it (show the counter offer instead)
          if (offer.status === 'countered') return false;
          return true;
        });
        setOffers(filteredOffers);
      }
      }
    } catch (error) {
      console.error('âŒ Load data error:', error);
    } finally {
      setLoading(false);
      setRefreshing(false);
    }
  };

  const onRefresh = () => {
