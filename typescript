[7m# Find where accepted status is defined and make sure actionable is true[27m[K
[7mcat > /tmp/fix_accepted_actionable.py << 'PYTHON'[27m[K
[7mimport re[27m[K
[K
[7mwith open('src/screens/MyNeedsScreen.js', 'r') as f:[27m[K
[7m    content = f.read()[27m[K
[K
[7m# Find the accepted status config and ensure actionable is true[27m[K
[7mold_accepted = r"(accepted: \{[^}]*label: 'Payment Pending'[^}]*actionable: )false"[27m[K
[7mnew_accepted = r"\1true"[27m[K
[K
[7mcontent = re.sub(old_accepted, new_accepted, content)[27m[K
[K
[7m# Also check if there's an 'Offer Accepted' status[27m[K
[7mold_offer_accepted = r"(label: 'Offer Accepted',[^}]*actionable: )false"[27m[K
[7mnew_offer_accepted = r"\1true"[27m[K
[K
[7mcontent = re.sub(old_offer_accepted, new_offer_accepted, content)[27m[K
[K
[7mwith open('src/screens/MyNeedsScreen.js', 'w') as f:[27m[K
[7m    f.write(content)[27m[K
[K
