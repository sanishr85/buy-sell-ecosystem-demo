      const user = userData ? JSON.parse(userData) : { id: 'seller-1' };
      
      const myOffers = demoOffers.filter(o => o.sellerId === user.id);
      console.log(`ðŸ“¤ Found ${myOffers.length} offers`);
      
      return { success: true, offers: myOffers };
    }
    
