[7mcat > /tmp/fix_offer_navigation.py << 'PYTHON'[27m[K
[7mimport re[27m[K
[K
[7mwith open('src/screens/MyOffersScreen.js', 'r') as f:[27m[K
[7m    content = f.read()[27m[K
[K
[7m# Find the navigation logic and update it[27m[K
[7mold_nav = r"onPress=\{\(\) => \{\s+navigation\.navigate\('OfferDetails', \{ offerId: offer\.id \}\);\s+\}\}"[27m[K
[K
[7mnew_nav = """onPress={() => {[27m[K
[7m                  // If offer has an order, navigate to order tracking[27m[K
[7m                  if (offer.orderId) {[27m[K
[7m                    navigation.navigate('SellerOrderTracking', { orderId: offer.orderId });[27m[K
[7m                  } else {[27m[K
[7m                    navigation.navigate('OfferDetails', { offerId: offer.id });[27m[K
[7m                  }[27m[K
[7m                }}"""[27m[K
[K
[7mcontent = re.sub(old_nav, new_nav, content)[27m[K
[K
[7mwith open('src/screens/MyOffersScreen.js', 'w') as f:[27m[K
[7m    f.write(content)[27m[K
[K
[7mprint("âœ… Updated offer navigation to check for orderId")[27m[K
