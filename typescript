[7m# Update ViewOffersScreen to filter and show payment button properly[27m[K
[7mcat > /tmp/fix_viewoffers_display.js << 'EOF'[27m[K
[7mconst fs = require('fs');[27m[K
[7mlet content = fs.readFileSync('src/screens/ViewOffersScreen.js', 'utf8');[27m[K
[K
[7m// Find the loadData function and update to filter countered offers[27m[K
[7mconst oldLoadData = /const loadData = async \(\) => \{[\s\S]*?if \(offersResponse\.success\) \{[\s\S]*?setOffers\(offersResponse\.offers \|\| \[\]\);/;[27m[K
[K
[7mconst newLoadData = `const loadData = async () => {[27m[K
[7m    try {[27m[K
[7m      // Load both need and offers[27m[K
[7m      const [needResponse, offersResponse] = await Promise.all([[27m[K
[7m        needsAPI.getById(needId),[27m[K
[7m        offersAPI.getByNeed(needId)[27m[K
[7m      ]);[27m[K
[K
[7m      if (needResponse.success) {[27m[K
[7m        setNeed(needResponse.need);[27m[K
[7m      }[27m[K
[K
[7m      if (offersResponse.success) {[27m[K
[7m        // Filter out original offers that have been countered[27m[K
[7m        // Show only: pending offers, accepted offers, declined offers, and counter offers[27m[K
[7m        const filteredOffers = (offersResponse.offers || []).filter(offer => {[27m[K
[7m          // If offer is countered, don't show it (show the counter offer instead)[27m[K
[7m          if (offer.status === 'countered') return false;[27m[K
[7m          return true;[27m[K
[7m        });[27m[K
[7m        setOffers(filteredOffers);[27m[K
[7m      }`;[27m[K
[K
[7mcontent = content.replace(oldLoadData, newLoadData);[27m[K
[K
[7mfs.writeFileSync('src/screens/ViewOffersScreen.js', content);[27m[K
[7mconsole.log('âœ… ViewOffersScreen updated to filter countered offers');[27m[K
[7mEOF[27m[K
