[7mold_pattern = r"(await needsAPI\.update\(offer\.needId, \{\s+status: 'in_progress',\s+orderId: newOrder\.id\s+\}\);)"[27m[K
[K
[7mnew_code = r"""\1[27m[K
[K
[7m      // âœ… Update the offer with orderId so seller can navigate to order tracking[27m[K
[7m      const { demoOffers } = require('./offers2');[27m[K
[7m      const offerIndex = demoOffers.findIndex(o => o.id === orderData.offerId);[27m[K
[7m      if (offerIndex !== -1) {[27m[K
[7m        demoOffers[offerIndex].orderId = newOrder.id;[27m[K
[7m        console.log('âœ… Offer updated with orderId:', newOrder.id);[27m[K
[7m      }"""[27m[K
[K
[7mcontent = re.sub(old_pattern, new_code, content)[27m[K
