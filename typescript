[7mcat > /tmp/update_both_offers.py << 'PYTHON'[27m[K
[7mimport re[27m[K
[K
[7mwith open('src/api/orders2.js', 'r') as f:[27m[K
[7m    content = f.read()[27m[K
[K
[7m# Find the offer update section and expand it to update both offers[27m[K
[7mold_code = r"const offerIndex = demoOffers\.findIndex\(o => o\.id === orderData\.offerId\);\s+if \(offerIndex !== -1\) \{\s+demoOffers\[offerIndex\]\.orderId = newOrder\.id;\s+console\.log\('âœ… Offer up[7md[7mated with orderId:', newOrder\.id\);\s+\}"[27m[K
[K
[7mnew_code = """const offerIndex = demoOffers.findIndex(o => o.id === orderData.offerId);[27m[K
[7m      if (offerIndex !== -1) {[27m[K
[7m        demoOffers[offerIndex].orderId = newOrder.id;[27m[K
[7m        [27m[K
[7m        // If this is a counter offer, also update the original offer[27m[K
[7m        const acceptedOffer = demoOffers[offerIndex];[27m[K
[7m        if (acceptedOffer.originalOfferId) {[27m[K
[7m          const originalIndex = demoOffers.findIndex(o => o.id === acceptedOffer.originalOfferId);[27m[K
[7m          if (originalIndex !== -1) {[27m[K
[7m            demoOffers[originalIndex].orderId = newOrder.id;[27m[K
[7m          }[27m[K
[7m        }[27m[K
[7m        [27m[K
[7m        console.log('âœ… Offer updated with orderId:', newOrder.id);[27m[K
[7m      }"""[27m[K
[K
[7mcontent = re.sub(old_code, new_code, content, flags=re.DOTALL)[27m[K
[K
[7mwith open('src/api/orders2.js', 'w') as f:[27m[K
[7m    f.write(content)[27m[K
[K
[7mprint("âœ… Updated to set orderId on both original and counter offers")[27m[K
